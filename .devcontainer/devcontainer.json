
{
    "name": "Python Container",
    "dockerFile": "Dockerfile",
    "forwardPorts": [8787], 
    "customizations": {
            "vscode": {
                    "extensions": [
                        "REditorSupport.r",
                        "RDebugger.r-debugger",
                        "Mikhail-Arkhipov.r",
                        "ms-python.python"
                    ]
            }
    },
    "runArgs": [ "--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined", "--gpus=all"]

}
