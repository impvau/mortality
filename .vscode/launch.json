    "version": "0.2.0",
    "configurations": [
        {
            "type": "R-Debugger",
            "name": "Debug compute_cfr.R",
            "request": "launch",
            "debugMode": "file",
            "workingDirectory": "${workspaceFolder}",
            "file": "${workspaceFolder}/src/compute_cfr.R"
        },
        {
            "name": "Run",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/run.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Pre Process",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/preproc.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Post Process",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/postproc.py",
            "console": "integratedTerminal"
        }
    ]
}